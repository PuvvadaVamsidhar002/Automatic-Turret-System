    "import cv2\n",
    "video=cv2.VideoCapture(0)\n",
    "while True:\n",
    "    ret, frame=video.read()\n",
    "    cv2.imshow('Live Feed',frame)\n",
    "    if(ord('q')==cv2.waitKey(10)) or (ord('Q')==cv2.waitKey(10)):\n",
    "        break\n",
    "video.release()"
